  "license": "MIT",
  "bugs": {
    "url": "https://github.com/go-zaru/node-package-template/issues"
  },
  "homepage": "https://github.com/go-zaru/node-package-template#readme",
  "devDependencies": {
    "@hapi/code": "^5.3.1",
    "@hapi/lab": "^19.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^12.0.7",
    "dotenv": "^8.0.0",
    "sinon": "^7.3.2",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.1"
  }
}
