{
  "private": true,
  "name": "@org/node-package-template",
  "version": "0.1.0",
  "description": "Add description here",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "build": "npm run clean && npm run lint && tsc && npm run copy:md",
    "copy:package": "cp package.json lib/package.json",
    "copy:readme": "cp README.md lib/README.md",
    "copy:changelog": "cp CHANGELOG.md lib/CHANGELOGE.md",
    "copy:md": "npm run copy:readme && npm run copy:changelog",
    "publish:lib": "npm run copy:package && npm publish ./lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/go-zaru/node-package-template.git"
  },
  "keywords": [
    "Template"
  ],
